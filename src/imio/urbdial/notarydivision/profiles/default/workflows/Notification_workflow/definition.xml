 <transition transition_id="Cancel"
             title="Cancel"
             new_state="Cancelled"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" category="workflow" i18n:translate="">Cancel</action>
  <guard>
    <guard-permission>imio.urbdial.notarydivision: Manage notification</guard-permission>
  </guard>
 </transition>
 <transition transition_id="Notify"
             title="Notify"
             new_state="In investigation"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" category="workflow" i18n:translate="">Notify</action>
  <guard>
    <guard-permission>imio.urbdial.notarydivision: Manage notification</guard-permission>
  </guard>
 </transition>
 <transition transition_id="Pass"
             title="Pass"
             new_state="Passed"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" category="workflow" i18n:translate="">Pass</action>
  <guard>
    <guard-permission>imio.urbdial.notarydivision: Manage notification</guard-permission>
  </guard>
 </transition>
 <transition transition_id="Restart"
             title="Restart"
             new_state="In preparation"
             trigger="USER"
             before_script=""
             after_script=""
             i18n:attributes="title">
  <action url="" category="workflow" i18n:translate="">Restart</action>
  <guard>
    <guard-permission>Manage portal</guard-permission>
  </guard>
 </transition>
 <variable variable_id="action" for_catalog="False"
           for_status="True" update_always="True">
  <description>The last transition</description>
  <default>
   <expression>transition/getId|nothing</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="actor" for_catalog="False"
           for_status="True" update_always="True">
  <description>The ID of the user who performed the last transition</description>
  <default>
   <expression>user/getId</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="comments" for_catalog="False"
           for_status="True" update_always="True">
  <description>Comments about the last transition</description>
  <default>
   <expression>python:state_change.kwargs.get('comment', '')</expression>
  </default>
  <guard>
  </guard>
 </variable>
 <variable variable_id="review_history" for_catalog="False"
           for_status="False" update_always="False">
  <description>Provides access to workflow history</description>
  <default>
   <expression>state_change/getHistory</expression>
  </default>
  <guard>
   <guard-permission>Request review</guard-permission>
   <guard-permission>Review portal content</guard-permission>
  </guard>
 </variable>
 <variable variable_id="time" for_catalog="False"
           for_status="True" update_always="True">
  <description>Time of the last transition</description>
  <default>
   <expression>state_change/getDateTime</expression>
  </default>
  <guard>
  </guard>
 </variable>
