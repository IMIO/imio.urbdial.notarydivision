<?xml version="1.0"?>
<rolemap>
 <roles>
    <role name="NotaryDivision Creator"/>
    <role name="Observation Creator"/>
    <role name="Observation Manager"/>
 </roles>
 <permissions>
     <permission
         name="Add portal content"
         acquire="True">
         <role name="NotaryDivision Creator" />
     </permission>
     <permission
         name="imio.urbdial.notarydivision: Add Observation"
         acquire="True">
         <role name="Observation Creator" />
     </permission>
     <permission
         name="imio.urbdial.notarydivision: Manage Observation"
         acquire="True">
         <role name="Observation Manager" />
     </permission>
 </permissions>
</rolemap>
