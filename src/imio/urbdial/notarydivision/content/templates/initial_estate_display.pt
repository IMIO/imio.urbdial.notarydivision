<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="urbdial.divnot"
      tal:omit-tag="">

    <table class="datagridwidget-table-view">
        <thead>
            <tr>
                <th class="header">
                    <span i18n:translate="">Locality</span>
                </th>
                <th class="header">
                    <span i18n:translate="">Cadastral reference</span>
                </th>
                <th class="header">
                    <span i18n:translate="">Surface</span>
                </th>
                <th class="header specific_rights">
                    <span i18n:translate="">Specific rights</span>
                </th>
            </tr>
        </thead>
        <tbody id="datagridwidget-tbody" tal:attributes="data-name_prefix view/name_prefix; data-id_prefix view/id_prefix">
            <tal:block repeat="line view/value">
                <tr>
                    <td tal:replace="structure python:view.display_initial_state_line(line)"></td>
                </tr>
            </tal:block>
        </tbody>
    </table>
</html>
