<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    i18n_domain="urbdial.divnot">

  <!-- values -->
  <adapter
     for="zope.interface.Interface
          zope.publisher.interfaces.browser.IBrowserRequest
          imio.urbdial.notarydivision.browser.interfaces.ICreatedParcelTable"
     factory=".table.CreatedParcelValues"
     provides="z3c.table.interfaces.IValues"
  />

  <!-- columns -->
  <adapter
     for="zope.interface.Interface
          zope.publisher.interfaces.browser.IBrowserRequest
          imio.urbdial.notarydivision.browser.interfaces.IParcelTable"
     factory=".table.ParcelNumberColumn"
     provides="z3c.table.interfaces.IColumn"
     name="parcelnumberColumn"
  />

  <adapter
     for="zope.interface.Interface
          zope.publisher.interfaces.browser.IBrowserRequest
