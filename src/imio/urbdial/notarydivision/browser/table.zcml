     factory=".table.BuiltColumn"
     provides="z3c.table.interfaces.IColumn"
     name="builtColumn"
  />

  <adapter
     for="zope.interface.Interface
          zope.publisher.interfaces.browser.IBrowserRequest
          imio.urbdial.notarydivision.browser.interfaces.ICreatedParcelTable"
     factory=".table.UndividedColumn"
     provides="z3c.table.interfaces.IColumn"
     name="undividedColumn"
